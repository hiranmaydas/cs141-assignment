#include <iostream>
#include <cmath>
using namespace std;
int nth(int a, int i){
	int b=  (a/pow (10,i-1));
	return b%10;
}
int check(int a) {
	int l,i,k=0;
	for (i=0; i<14; i++){
		l=nth(a,i);
		k=k+l*l*l*l;
	}
	if (k==a){
		return 0;
	}
	else return 1;
}
int main(){
	cout <<"enter a number "<<endl;
	int a,n,i;
	cin>>a;
	cin>>n;
	cout <<"the amstrong numbers from "<<a<<" to "<<n<<" is "<<endl;
	for (i=a; i<=n;i++){
 
		int u=1;
		u=check(i);
		if (u==0){
			cout<<i<<endl;
		}
	}
}
